{
    "welcomeFile": "/index.html",
    "authenticationMethod": "route",
    "routes": [
      {
        "source": "/manage/(.*)$",
        "target": "/manage/$1",
        "authenticationType": "xsuaa",
        "destination": "srv_api",
        "csrfProtection": true
      },
      {
        "source": "/review/(.*)$",
        "target": "/review/$1",
        "authenticationType": "xsuaa",
        "destination": "srv_api",
        "csrfProtection": true
      },
      {
        "source": "^/(.*)$",
        "target": "$1",
        "localDir": "webapp"
      }
    ]
  }